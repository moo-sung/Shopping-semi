<%@page import="dao.ProductDAO"%>
<%@page import="dto.ProductDTO"%>
<%@page import="java.util.List"%>
<%@ page language="java" contentType="text/html; charset=UTF-8"
	pageEncoding="UTF-8"%>
<%
	List<ProductDTO> productList=ProductDAO.getDAO().selectAllProductList("idx");
%>
<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>관리자</title>
<link rel="stylesheet"
	href="<%=request.getContextPath()%>/css/common.css">
<link rel="icon" href="<%=request.getContextPath()%>/images/favicon.ico">
<style type="text/css">
/* CSS 작성 */
img {
	width: 400px;
	height: 500px;
}

#container .main {
	/* 글 정렬 */
	text-align: center;
}

#container .mainImg li {
	display: inline;
}

#container .mainImg img {
	margin: 10px;
}

h1 {
	text-align: center;
}

table {
	margin: 5px auto;
	border: 0.5px solid rgb(207, 207, 207);
	width: auto;
	border-collapse: collapse;
}

th, td {
	padding: 11px 50px 10px;
	height: 40px;
	border: 0.5px solid rgb(207, 207, 207);
	text-align: center;
}
</style>
</head>
<body>
	<div id="wrap">
		<%-- side 불러오기 --%>
		<jsp:include page="/admin/admin_side.jsp" />
		<!-- container start -->
		<div id="container">
			<!-- content area => 본문작성 -->
			<div id="content">
				<div class="controlArea">
					<h1>게시판관리</h1>
				</div>
				<div class="noticelist">
					<form class="noticeTable">
						<table>
							<tr>
								<th>이메일</th>
								<th>상품코드</th>
								<th>제목</th>
								<th>게시글</th>
								<th>이미지</th>
								<th>등록일</th>
								<th>조회수</th>
							</tr>
							
						</table>
					</form>
				</div>
			</div>
		</div>
	</div>
</body>
</html>