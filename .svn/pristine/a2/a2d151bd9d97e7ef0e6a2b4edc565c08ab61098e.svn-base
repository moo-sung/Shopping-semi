<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>nookyoon</title>
<link href="https://fonts.googleapis.com/css2?family=Roboto:ital,wght@0,100;0,300;0,400;0,500;0,700;0,900;1,100;1,300;1,400;1,500;1,700;1,900&display=swap" rel="stylesheet">
<link rel="stylesheet" href="<%=request.getContextPath() %>/css/common.css"> 
<link rel="icon" href="<%=request.getContextPath()%>/images/favicon.ico">
<style type="text/css">
/* CSS 작성 */
#navi {
	text-align: center;
}
#navi li {
	display: inline-block;
	font-size: 12px;
	margin: 15px;
}
#orderList {
	margin: 0 auto;
	border: 1px solid white;
	width: 800px;
}
#orderList .title {
	border-bottom: 1px solid white;
	text-align: center;
}
#orderList .first {
	text-align: center;
	vertical-align: middle;
}
#orderList .first>li {
	display: inline-block;
}
</style>
</head>
<body>
  <div id="wrap">

    <!-- container start -->  
    <div id="container">
	   <!-- content area => 안에 내용들 다 지우고본문작성 -->
      <div id="content">   
        <div class="main"> 
        	<div class="titleArea">
        		<h1>My Page</h1>
        		<br>
         	</div>  
         	<div id="navi">
         		<ul>
         			<li>테스트</li>
         			<li>테스트</li>
         			<li>테스트</li>
         			<li>테스트</li>         			
         		</ul>
         	</div>
        	
        	<div id="button">
        	
        	</div>
        	
        	<div id="orderList">
        		<div class="title">
	        		<h3>나의 주문처리 현황</h3>
        		</div>
    				<ul class="first">
    					<li>테스트</li>
    					<li>테스트</li>
    					<li>테스트</li>
    					<li>테스트</li>
    					<li>
    						<ul class="twice">
    							<li>테스트</li>
    							<li>테스트</li>
    							<li>테스트</li>
    						</ul>
    					</li>
    				</ul>    		
        	</div>
        </div>
      </div>
    </div>
    <!-- container end -->

  </div>
</body>
</html>